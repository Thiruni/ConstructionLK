
@using Microsoft.Ajax.Utilities
@using Microsoft.AspNet.Identity

@model ConstructionLK.Models.RatingsCustomer

@{
    ViewBag.Title = "showRatingsForServiceProvider";
}

<h2>Ratings for your service</h2>
@if (ViewBag.AvgRatings==null)
{
    <p>We don't have any customers yet.</p>
}
else
{
    <h3> @Html.Raw(ViewBag.AvgRatings)</h3>
}
  



